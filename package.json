{
  "name": "todovs",
  "displayName": "todovs",
  "description": "enables you to keep track of all your tasks",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "todovs-sidebar-view",
          "title": "todoVS",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "todovs-sidebar-view": [
        {
          "type": "webview",
          "id": "todovs-sidebar",
          "name": "todoVS",
          "icon": "media/checklist.svg",
          "contextualTitle": "VSinder"
        }
      ]
    },
    "commands": [
      {
        "command": "todovs.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "todovs.refresh",
        "category": "TodoVS",
        "title": "Refresh"
      },
      {
        "command": "todovs.addToDo",
        "category": "TodoVS",
        "title": "Add Todo from any selection"
      },
      {
        "command": "todovs.authenticate",
        "category": "TodoVS",
        "title": "Authenticate "
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
